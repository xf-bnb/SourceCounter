{
    "Java": {
        "extensions": [".java"],
		"analyzer": "C",
        "syntax": [ ["//"],
                    [["/**", "**/"]],
                    [["\"", "\""]],
                    [] 
                  ]
    },
	"C/C++ Header": {
        "extensions": [".h"],
		"analyzer": "C"
    },
	"C": {
		"analyzer": "C++",
		"syntax": [ ["#"],
                    [["<!--", "-->"]],
                    [["\"", "\""]],
                    [["a" "b"]] 
                  ]
    },
	"C++": {
        "extensions": [".cpp", ".cxx"]
    },
	"Ruby": {
		"syntax": [ ["#"],
                    [["=begin", "=end"]],
                    [["%q{", "}"]],
                    [["%Q[", "]"]] 
                  ]
    }
}
