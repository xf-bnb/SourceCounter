{
    "Java": {
        "extensions": [".java"],
		"analyzer": "C",
        "syntax": [ ["//"],
                    [["/**", "**/"]],
                    [["\"", "\""]],
                    [] 
                  ]
    },
	"C": {
		"analyzer": "C++",
		"syntax": [ ["#"],
                    [["<!--", "-->"]],
                    [["\"", "\""]],
                    [["a", "b"]] 
                  ]
    },
	"C++": {
        "extensions": [".cpp", ".inl"]
    },
	"C++ Header": {
        "extensions": [".h"],
		"analyzer": "C++"
    },
	"Ruby": {
		"syntax": [ ["#"],
                    [["=begin", "=end"]],
                    [["%q{", "}"]],
                    [["%Q[", "]"]] 
                  ]
    },
    "Frank": {
        "extensions": [".cs"]
    }
}
